pipeline {
   agent any
   tools {
     maven "maven"  
   }
   stages {
      stage('Git Checkout') {
         steps {
            git 'https://github.com/srikanta1219/srikanta-jenkins-docker-multiple-jdk.git'   
         }
      }
    
    stage ('Maven Goal'){
      steps {
         sh "mvn clean install package    " 
      }    
    }


    }
}
